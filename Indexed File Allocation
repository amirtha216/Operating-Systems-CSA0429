#include <stdio.h>

int main() {
    int n, i;
    int indexBlock[50];

    printf("Enter number of blocks in file: ");
    scanf("%d", &n);

    printf("Enter block numbers:\n");
    for(i = 0; i < n; i++)
        scanf("%d", &indexBlock[i]);

    printf("\nIndexed File Allocation:\n");
    printf("Index Block Entries:\n");

    for(i = 0; i < n; i++)
        printf("Index[%d] -> Block %d\n", i, indexBlock[i]);

    return 0;
}
